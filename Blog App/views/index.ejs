<% include ./partials/header.ejs %>

<h1>Blog Page</h1>
<% blogs.forEach(blog =>{%>
    <div class = "container">
        <div class="jumbotron">
            <h3><%= blog.title %></h3>
            <img src="<%= blog.image %>">
            <span><%= blog.created.toDateString() %></span>
            <p><%= blog.description %></p>
            <a href="/blogs/<%=blog._id%>">See More</a>
        </div>
    </div>
<% }); %>

<% include ./partials/footer.ejs %>